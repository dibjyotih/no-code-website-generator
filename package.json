    "cheerio": "^1.1.2",
    "chromadb": "^3.0.17",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "hnswlib-node": "^3.0.0",
    "langchain": "^0.3.36",
    "multer": "^2.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "uuid": "^13.0.0"
